@model IList<CheeseMVC.Models.CheeseCategory>

<h1>@ViewBag.Title</h1>

<table class="table">

    <tr>
        <th>Name</th>

    </tr>

    @foreach (var Category in Model)
    {
        <tr>
  
            <td>@Category.Name</td>
        </tr>
    }
</table>


<p><a asp-controller="Category" asp-action="Add">Add Category</a></p>